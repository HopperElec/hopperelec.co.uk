{
	"$schema": "https://biomejs.dev/schemas/1.7.0/schema.json",
	"vcs": {
		"enabled": true,
		"clientKind": "git",
		"useIgnoreFile": true
	},
	"files": {
		"ignore": ["./package-lock.json"]
	},
	"linter": {
		"rules": {
			"style": {
				"noParameterAssign": "off",
				"useConst": "off"
			}
		}
	}
}
